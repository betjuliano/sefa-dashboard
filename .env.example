# Rename this file to .env and fill the values
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Optional: default goal (meta) for Likert mean (1-5)
DEFAULT_GOAL=4.0
# Storage backend options
# STORAGE_ROOT: base dir for local storage (default: data)
STORAGE_ROOT=
# FORCE_LOCAL_STORAGE: set to 'true' to ignore Supabase and always use local storage
FORCE_LOCAL_STORAGE=
# Streamlit secret key fallback (optional)
STREAMLIT_SECRET_KEY=
